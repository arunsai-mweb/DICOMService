<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<link rel="icon" href="~/favicon.png">
	<title>@ViewData["Title"]</title>
	<script src="~/lib/jquery/dist/jquery.js"></script>
	<script src="~/lib/jquery/dist/bootstrap.min.js"></script>
	<script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
	<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
	<script src="https://cdn.datatables.net/1.10.20/js/jquery.dataTables.min.js"></script>
	<script src="https://cdn.datatables.net/1.10.20/js/dataTables.bootstrap4.min.js"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
	<script src="~/js/Dialogs.js"></script>
	<script src="~/js/site.js"></script>
	<script src="~/lib/bootstrap/dist/js/bootstrap-notify.js"></script>
	<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
	<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
	<link href="~/lib/bootstrap/dist/css/bootstrap-notify.css" rel="stylesheet" />
	<link rel="stylesheet" href="~/css/site.css" />
	<link href="~/css/daterangepicker.css" rel="stylesheet" />
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
	<link rel="stylesheet" href="https://cdn.datatables.net/1.10.20/css/dataTables.bootstrap4.min.css" />
	<script src="~/lib/DataTable.Rowgroup.plugin.js"></script>
	<script src="~/lib/moment.min.js"></script>
	<script src="~/lib/daterangepicker.js"></script>
</head>
<body>
	<nav class="navbar navbar-expand-md navbar-dark" style="background-color:#0a82d4">
		<button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
			<span class="navbar-toggler-icon"></span>
		</button>
		<a class="navbar-brand nav-anch ulTabs" style="font-size:25px" asp-action="Home" asp-controller="Home">
			<i class="fa fa-home" title="home"></i>
		</a>
		<a asp-action="Home" asp-controller="Home" title="Main Menu" aria-expanded="false" class="navbar-brand nav-anch"><span class="menu-collapsed">DICOM SERVICE</span></a>
		<div align="right" style="margin-left:800px">
		<a asp-action="Clients" asp-controller="Home" aria-expanded="false" class="navbar-brand ulTabs nav-menu tab2"><i class="fa fa-users" style="margin-right:10px"></i>Clients</a>
		<a asp-action="Destinations" asp-controller="Home" aria-expanded="false" class="navbar-brand nav-menu ulTabs tab3"><i class="fa fa-map-marker" style="margin-right:10px"></i>Destinations</a>
		<a asp-action="Index" asp-controller="Home" aria-expanded="false" class="navbar-brand ulTabs nav-menu tab4"><i class="fa fa-plus-square" style="margin-right:10px"></i>Cases</a>
		<a asp-action="GetLogs" asp-controller="Home" aria-expanded="false" class="navbar-brand ulTabs nav-menu tab5"><i class="fa fa-history" style="margin-right:10px"></i>Logs</a>
        </div>
		<div class="collapse navbar-collapse" id="navbarNavDropdown">
			<ul class="navbar-nav ml-auto ulLogIn">
				<li class="nav-item dropdown dropdown-user open">
					<span style="color:#fff;font-size:20px;font-weight:bold;margin-right:10px">@DiagoDICOM.Common.AppUser.UserName</span>
					<a data-toggle="dropdown" aria-expanded="true">
						<i class="fa fa-user-circle-o user" style="color:#fff;font-size:25px"></i>
					</a>
					<ul class="dropdown-menu dropdown-menu-right" style="padding: 3px;background-color: #ededed;margin-top:13px">
						<li style="margin-left:15px;width:175px">
							<a asp-action="LogOut" asp-controller="Account" style="color:#333;font-size:15px;text-decoration:none">
								<i class="fa fa-sign-out" style="margin-right:10px"></i>Sign Out
							</a>
						</li>
						<li style="margin-left:15px">
							<a asp-action="ChangePassword" asp-controller="Account" style="color:#333;font-size:15px;text-decoration:none">
								<i class="fa fa-key" style="margin-right:10px"></i>
								Change Password
							</a>
						</li>
					</ul>
				</li>
			</ul>
		</div>
	</nav>
		@*<div class="col-xs-12 col-lg-12 col-sm-12 col-md-12">
			<ul class="nav multilinks nav-tabs nav-justified ulTabs" style="margin-top:10px">				
				<li class="nav-item">
				</li>
				<li class="nav-item">
				</li>
				<li class="nav-item">
				</li>
				<li class="nav-item">
				</li>
			</ul>
		</div>*@
	<div class="row" id="body-row">
		<div class="col py-3">
			@RenderBody()
		</div>
		<div class="modal fade" id="modalDialog">
			<div class="modal-dialog">
				<div class="modal-content">
					<div class="modal-header">
						<h4 class="modal-title"></h4>
						<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
					</div>
					<div class="modal-body">
					</div>
				</div>
			</div>
		</div>
	</div>
</body>
</html>

